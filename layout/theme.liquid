<script>
  function waitForElm(selector) {
      return new Promise(resolve => {
          if (document.querySelector(selector)) {
              return resolve(document.querySelector(selector));
          }

          const observer = new MutationObserver(mutations => {
              if (document.querySelector(selector)) {
                  resolve(document.querySelector(selector));
                  observer.disconnect();
              }
          });

          observer.observe(document.body, {
              childList: true,
              subtree: true
          });
      });
  }

  function changePriceQuantity() {

      setTimeout(function() {
          let currentQuantity = document.querySelector('.quantity-input').value;
          console.log(document.querySelector('.modal_price .sale_price') !== null)
          if (document.querySelector('.modal_price .sale_price') !== null) {

              let salePrice = document.querySelector('[data-price]');
              let originalPrice = document.querySelector('[data-compare-price]')

              salePrice.innerHTML = '$' + (parseInt(salePrice.getAttribute('data-price')) * currentQuantity) / 100 + "&nbsp;";
              originalPrice.innerHTML = '$' + (parseInt(originalPrice.getAttribute('data-compare-price')) * currentQuantity) / 100;
              console.log(originalPrice.innerHTML)
              document.querySelector('.modal_price [data-price-mutation-watcher]').style.display = 'none';
              try {
                  document.querySelector('.modal_price s').removeChild(document.querySelector('.modal_price s .original-price-child'))
              } catch {
                  console.log('no adjusted prices yet')
              }
              let newSpan = document.createElement('span')
              newSpan.classList.add('original-price-child')
              newSpan.innerHTML = '$' + ((parseInt(originalPrice.getAttribute('data-compare-price')) * currentQuantity) / 100).toFixed(2);
              document.querySelector('.modal_price s').appendChild(newSpan)
              let shopifyMeta = document.querySelector('shopify-payment-terms').getAttribute('shopify-meta')
              let parsedJSON = JSON.parse(shopifyMeta)
              let currentInterest = '$' + (parseInt(parsedJSON.variants[0].price.replace(/\$/g, '')) * currentQuantity)
              document.querySelector('.payment-price').innerHTML = currentInterest;
              let hostCart = document.querySelector('.shoppay-payment [shopify-meta]')
              let styleCart = document.createElement('style')
              styleCart.textContent = '.shopify-installments__content{display:none!important;}'
              hostCart.shadowRoot.appendChild(styleCart)
              document.querySelector('.shoppay-payment .payment-text').style.display = 'block';
              document.querySelector('.shoppay-payment .shoppay-logo').style.display = 'block';
          } else {

              let salePrice = document.querySelector('[data-price]')
              let originalPrice = document.querySelector('[data-price]')
              salePrice.innerHTML = '$' + (parseInt(salePrice.getAttribute('data-price')) * currentQuantity) / 100 + "&nbsp;";
              originalPrice.innerHTML = '$' + (parseInt(originalPrice.getAttribute('data-price')) * currentQuantity) / 100;
              console.log(originalPrice.innerHTML)
              try {
                  document.querySelector('.modal_price [data-price-mutation-watcher]').style.display = 'none';
              } catch {

                  if (document.querySelector('.modal_price[data-price-mutation-watcher]') !== null) {
                      document.querySelector('.modal_price[data-price-mutation-watcher]').style.display = 'none';
                  } else {
                      document.querySelector('p.modal_price').style.display = 'none';
                  }
              }



              try {
                  document.querySelector('.modal_price').parentElement.removeChild(document.querySelector('.original-price-child'))
              } catch {
                  console.log('no adjusted prices yet')
              }
              try {
                  let newSpan = document.createElement('span')
                  newSpan.classList.add('original-price-child')
                  newSpan.innerHTML = '$' + ((parseInt(originalPrice.getAttribute('data-price')) * currentQuantity) / 100).toFixed(2);
                  document.querySelector('.modal_price').parentElement.appendChild(newSpan)
              } catch {
                  console.log('no sale price')
              }

              let shopifyMeta = document.querySelector('shopify-payment-terms').getAttribute('shopify-meta')
              let parsedJSON = JSON.parse(shopifyMeta)
              let currentInterest = '$' + (parseInt(parsedJSON.variants[0].price.replace(/\$/g, '')) * currentQuantity)
              document.querySelector('.payment-price').innerHTML = currentInterest;
              let hostCart = document.querySelector('.shoppay-payment [shopify-meta]')
              let styleCart = document.createElement('style')
              styleCart.textContent = '.shopify-installments__content{display:none!important;}'
              hostCart.shadowRoot.appendChild(styleCart)
              document.querySelector('.shoppay-payment .payment-text').style.display = 'block';
              document.querySelector('.shoppay-payment .shoppay-logo').style.display = 'block';

          }

      }, 1200)
  }

  document.addEventListener('DOMContentLoaded', function() {

      function populateText(allUpsell) {
          for (let a = 0; a < allUpsell.length; a++) {
              let productHref = allUpsell[a].querySelector('.rebuy-product-info a').href;
              productHref = productHref + '&view=metafield'
              console.log(productHref)
              fetch(productHref).then(response => response.text()).then(data => {
                  const parser = new DOMParser();
                  const htmlDocument = parser.parseFromString(data, "text/html");
                  let jsonMetafield = JSON.parse(htmlDocument.getElementById('product-metafield').innerHTML);
                  let newElement = document.createElement('span');
                  let newElement2 = document.createElement('span')
                  newElement.innerText = jsonMetafield.Hotel;
                  newElement.classList.add('rebuy-product-info__javsacript-span')
                  allUpsell[a].querySelector('.rebuy-product-info').appendChild(newElement);
                  newElement2.innerText = jsonMetafield.Scent;
                  newElement2.classList.add('rebuy-product-info__javsacript-span')
                  allUpsell[a].querySelector('.rebuy-product-info').appendChild(newElement2);
              });
          }
      }
      waitForElm('#rebuy-widget-42751 .rebuy-product-grid .rebuy-product-info').then((elm) => {
          populateText(document.querySelectorAll('#rebuy-widget-42751 .rebuy-product-block'));
          populateText(document.querySelectorAll('#rebuy-widget-24588 .rebuy-product-block'));
      });
  })


