  if (isMobile) {
      waitForElm(".rebuy-cart__flyout .dynamic-checkout__content undefined").then((elm) => {
          const para = elm.querySelector(".shopify-cleanslate");
          para.classList.add("para_collapsed");
          para.addEventListener("click", function() {
              if (para.classList.contains('para_collapsed')) {
                  para.classList.remove("para_collapsed");
              } else {
                  para.classList.add("para_collapsed");
              }
          })
